                                                        <button class="btn_mass_migration_warning" onclick="removeBatchID(<%=merkle._id%>,<%=merkle.batchId%>)">
                                                            Remove
                                                        </button>
                                                    </a>
                                                </td>
                                            <%}else{%>
                                                <td>
                                                    <p></p>
                                                </td>
                                            <%}%>
                                        </tr>
                                        <%});%>
                                </table>
                            </div>

