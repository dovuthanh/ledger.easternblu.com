<h3>Verify</h3>
<div class="form_verify" id="form_verify">
	<p>Enter the song registration address</p>
	<div id="registration-info"></div>
	<form id="form_verify_songs" action="" onsubmit="return false;">
		<div class="form-group">
			<div class="col-md-12 col-xs-12">
				<input type="text" name="registration_address" id="registration_address" class="form-control" placeholder="" tabindex="0"  value="<%=address%>">
			</div>
		</div>
		<div class="form-group" hidden="">
			<div class="col-md-12 col-xs-12">
				<input type="text" name="hash_of_song" id="hash_of_song" class="form-control" placeholder="" tabindex="0"  value="<%=hashOfSong%>">
			</div>
		</div>

		<div class="form-group">
			<div class="col-md-12 col-xs-12">
				<button type="submit" class="btn btn-primary submit" onclick="Verify.verify()">VERIFY</button>
			</div>
		</div>
	</form>

	<div class="clear"></div>
    <%- include ../process/loading.ejs %>
</div>