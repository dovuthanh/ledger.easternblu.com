<h3>Authenticate</h3>
<!-- <p>Enter the song registration address</p> -->
<div class="form_verify" id="form_authentication">
	<form id="form_verify_songs" action="" onsubmit="return false;">
		<div class="form-group">
			<label class="col-md-4 control-label">Hash</label>
			<div class="col-md-12 col-xs-12">
				<input type="text" name="auth_hash_of_song" id="auth_hash_of_song" class="form-control"  tabindex="1"  value="<%=hash%>">
			</div>
		</div>
		<!--<div class="form-group">-->
			<!--<label class="col-md-4 control-label">Merkle Root (If any)</label>-->
			<!--<div class="col-md-12 col-xs-12">-->
				<!--<input type="text" name="auth_merkle_root" id="auth_merkle_root" class="form-control"  tabindex="1"  value="<%=merkleRoot%>">-->
			<!--</div>-->
		<!--</div>-->
		<div class="form-group">
			<label class="col-md-4 control-label">Digital Signature</label>
			<div class="col-md-12 col-xs-12">
				<input type="text" name="auth_digital_signatures" id="auth_digital_signatures" class="form-control" tabindex="0"  value="<%=signature%>">
			</div>
		</div>

		<div class="form-group">
			<label class="col-md-4 control-label">Account Number</label>
			<div class="col-md-12 col-xs-12">
				<input type="text" name="auth_account_number" id="auth_account_number" class="form-control" tabindex="2"  value="<%=role%>">
			</div>
		</div>

		<div class="form-group">
			<div class="col-md-12 col-xs-12">
				<button type="submit" class="btn btn-primary authenticate_submit" onclick="Verify.authentication();">AUTHENTICATE</button>
			</div>
		</div>
	</form>

	<div class="clear"></div>
    <%- include ../process/loading.ejs %>
</div>